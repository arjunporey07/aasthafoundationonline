database_name: aastha
	table name: events
		table col: 
			1. id(primary key:int)
			2. event_title(varchar:255)
			3. event_desc(text)
			4. event_img(varchar:255)
			5. event_dt(datetime)
			6. event_status(enam:active,inactive,deleted)
	
	
	table name: memberlist
		table col: 
			1. id(primary key:int)
			2. member_name(varchar:255)
			3. member_img(varchar:255)
			4. member_designation(text)
			5. member_position(text)
			6. member_quote(text)
			7. member_id_link
				a. facebook_link
				b. instagram_link
				c. twitter_link
			8. member_join_dt(datetime)
			9.member_status(enam:active,inactive,deleted)
			
	table name: new_activity
		table col: 	
			1. id(primary key:int)
			2. new_event_img(varchar:255)
			3. new_event_title(varchar:255)
			4. new_event_details(text)
			5. new_event_location(varchar:255)
			6. new_event_route(varchar:255)
			7. new_event_dt(datetime)
			
			
			
			
			
			
			
